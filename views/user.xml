<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="users_reserve_act_window" model="ir.actions.act_window">
        <field name="name">users</field>
        <field name="res_model">food_reservation.users</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                خالیه برادر من، خالی!
            </p>
        </field>
    </record>
    <record id="users_food_form_view" model="ir.ui.view">
        <field name="name">food_reservation.form</field>
        <field name="model">food_reservation.users</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name_id"/>
                </group>

            </form>
        </field>
    </record>
    <record id="users_food_tree_view" model="ir.ui.view">
        <field name="name">food_reservation.tree</field>
        <field name="model">food_reservation.users</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name_id"/>
            </tree>
        </field>
    </record>


</odoo>